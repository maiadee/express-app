<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>All Flowers</title>
  </head>
  <body>
    <h1>All Flowers</h1>
    <ul>
      <% allFlowers.forEach((flower) => { %>
      <li>
        <a href="/flowers/<%= flower._id %>"> <%= flower.name %> </a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
